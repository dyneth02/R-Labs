findRoots <- function(a, b, c) {
root_1 <<- (-b + sqrt(b^2 - 4*a*c)) / 2*a
root_2 <<- (-b - sqrt(b^2 - 4*a*c)) / 2*a
roots <- list(root_1, root_2)
return(roots)
}
roots <- findRoots(1,2,-15)
print(paste("First root is ", roots[1]))
print(paste("Second root is ", roots[2]))
