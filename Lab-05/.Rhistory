# Question 04
divisible_by_3 <- function(k) {
count <- 0
for (num in 1:k) {
if (num %% 3 == 0 ) {
count <- count + 1
}
}
return(count)
}
print(paste("There are", divisible_by_3(25), "elements in this vector"))
